{
  "expo": {
    "name": "SproutBook",
    "slug": "SproutBook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "sproutbook",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E7E78"
    },
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.anonymous.SproutBook",
      "usesAppleSignIn": true,
      "entitlements": {
        "com.apple.developer.applesignin": ["Default"]
      },
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "SproutBook needs access to your photo library to upload child profile pictures.",
        "NSPhotoLibraryAddUsageDescription": "SproutBook needs access to save child profile pictures to your photo library.",
        "NSCameraUsageDescription": "SproutBook needs access to your camera to take child profile pictures and journal entries.",
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIRequiresFullScreen": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#2E7E78"
      },
      "package": "com.anonymous.SproutBook",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./src/app"
        }
      ],
      "expo-web-browser",
      "@react-native-google-signin/google-signin",
      "expo-secure-store",
      "expo-font",
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "sproutbook-d0c8f"
      }
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
